{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../dist/scripts",
    "rootDir": ".",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES2020",
    "lib": ["ES2020"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": false,
    "declaration": false,
    "skipLibCheck": true
  },
  "include": ["./start-prod.ts"]
}
