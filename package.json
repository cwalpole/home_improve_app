{
  "name": "home-improve-app",
  "version": "0.1.0",
  "packageManager": "pnpm@10.17.0",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "next dev -p 3001 --turbopack",
    "build": "prisma migrate deploy && next build",
    "start": "node .next/standalone/server.js",
